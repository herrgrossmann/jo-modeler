<jnlp
	spec="$jnlpspec"
	codebase="$$context"
	href="$$name">
	<information>
		<title>Modeler</title>
		<vendor>org.jowidgets.modeler</vendor>
	</information>
	<security>
		<all-permissions />
	</security>
	<resources>
    	<j2se version="1.6+" java-vm-args="-XstartOnFirstThread"/>
		$dependencies
		<property name="jnlp.server.url" value="$$context/remoting" />
	</resources>
	<application-desc main-class="$mainClass" />
</jnlp>
